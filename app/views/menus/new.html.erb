    <%= form_with model: @menu,url: room_menus_path(room_id: params[:room_id]), local: true do |form| %>
      <%= form.text_field :name, placeholder: "料理名" %>
      <%= form.text_area :item, placeholder: "食材" %>
      <%= form.text_area :cook, placeholder: "作り方", rows: "10" %>
      <%= form.submit "送信する"%>
      
    <%end%>
